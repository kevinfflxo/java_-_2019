class Employee {
    private String id;
    private String name;
    private char gender;

    Employee(String id, String name, char gender) {
        this.id = id;
        this.name = name;
        this.gender = gender;
    }

    void setID(String id) {
        this.id = id;
    }

    void setName(String name) {
        this.name = name;
    }

    void setChare(char gender) {
        this.gender = gender;
    }

    String getName() {
        return name;
    }

    String getId() {
        return id;
    }

    char getGender() {
        return gender;
    }
}
